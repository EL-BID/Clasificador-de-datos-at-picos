@{
    ViewBag.Title = "Hogar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <h1>Ficha#@ViewBag.Hogar.Num_ficha - Hogar#@ViewBag.Hogar.Ide_hogar</h1>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <a class="btn btn-primary" href="@Url.Action("Hogares", "Home", new { cod_mpio = ViewBag.Hogar.Cod_mpio })">Volver a lista de hogares</a>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <h2><b>Secci&oacute;n B.</b> Datos de la vivienda</h2>
    </div>
</div>

<div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Tip_vivienda)">
    <div class="col-md-12">
        <label>17. Tipo de vivienda:</label>
        <span>@ViewBag.Vivienda.Tip_vivienda</span>
    </div>
</div>

<div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Tip_mat_paredes)">
    <div class="col-md-12">
        <label>18. Material predominante de las paredes exteriores:</label>
        <span>@ViewBag.Vivienda.Tip_mat_paredes</span>
    </div>
</div>

<div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Tip_mat_pisos)">
    <div class="col-md-12">
        <label>19. Material predominante de los pisos:</label>
        <span>@ViewBag.Vivienda.Tip_mat_pisos</span>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <label>20. &iquest;Con cu&aacute;les de los siguientes servicios p&uacute;blicos, privados o comunales cuenta la vivienda?</label>
        <table class="table table-condensed table-bordered">
            <tbody>
                <tr>
                    <th>A. Energ&iacute; el&eacute;ctrica</th>
                    <td><div class="@Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Ind_tiene_energia)">@ViewBag.Vivienda.Ind_tiene_energia (Estrato: @ViewBag.Vivienda.Tip_estrato_energia)</div></td>
                </tr>
                <tr>
                    <th>B. Alcantarillado</th>
                    <td><div class="@Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Ind_tiene_alcantarillado)">@ViewBag.Vivienda.Ind_tiene_alcantarillado</div></td>
                </tr>
                <tr>
                    <th>C. Gas natural domiciliario</th>
                    <td><div class="@Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Ind_tiene_gas)">@ViewBag.Vivienda.Ind_tiene_gas</div></td>
                </tr>
                <tr>
                    <th>D. Recolecci&oacute;n de basuras</th>
                    <td><div class="@Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Ind_tiene_recoleccion)">@ViewBag.Vivienda.Ind_tiene_recoleccion</div></td>
                </tr>
                <tr>
                    <th>E. Acueducto</th>
                    <td><div class="@Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Ind_tiene_acueducto)">@ViewBag.Vivienda.Ind_tiene_acueducto (Estrato: @ViewBag.Vivienda.Tip_estrato_acueducto)</div></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

<div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(ViewBag.ViviendaLog.Num_cuartos_vivienda)">
    <div class="col-md-12">
