<div class="row">
    <div class="col-md12">
        <ul class="nav nav-tabs">
            @foreach (var p in ViewBag.Personas)
            {
                <li @{ WriteLiteral(p.Ide_persona == 1 ? " class=\"active\"" : ""); }>
                    <a data-toggle="tab" href="#persona_@p.Ide_persona">
                        @p.Ide_persona. @{ Write(string.Join(" ", new string[] { p.Pri_nombre, p.Seg_nombre, p.Pri_apellido, p.Seg_apellido })); }
                    </a>
                </li>
            }
        </ul>

        <div class="tab-content panel panel-default">
            @foreach (Sisben.WebApps.QAML.Models.CNS_PERSONAS p in ViewBag.Personas)
            {
                var scores = ((Sisben.WebApps.QAML.Models.CNS_PERSONAS_Logs[])ViewBag.PersonasLog).Where(x => x.Ide_persona == p.Ide_persona).First();
                <div id="persona_@p.Ide_persona" class="tab-pane fade in@{ WriteLiteral(p.Ide_persona == 1 ? " active" : ""); } panel-body">
                    <div class="row">
                        <div class="col-md-12">
                            <h2><b>Secci&oacute;n D.</b> Antecedentes sociodemogr&aacute;ficos</h2>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <label>45. N&uacute;mero de orden:</label>
                            <span>@p.Ide_persona</span>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <label>46. Apellidos completos:</label>
                            <span>@{ Write($"{p.Pri_apellido} {p.Seg_apellido}".Trim()); }</span>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <label>47. Nombres completos:</label>
                            <span>@{ Write($"{p.Pri_nombre} {p.Seg_nombre}".Trim()); }</span>
                        </div>
                    </div>

                    <div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(@scores.Sexo_persona)">
                        <div class="col-md-12">
                            <label>48. Sexo:</label>
                            <span>@p.Sexo_persona</span>
                        </div>
                    </div>

                    <div class="row @Sisben.WebApps.QAML.Helpers.Alert.GetClass(@scores.Tip_documento)">
                        <div class="col-md-12">
                            <label>49. Tipo de documento de identidad:</label>
                            <span>@p.Tip_documento</span>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <label>50. N&uacute;mero de documento de identidad:</label>
                            <span>@p.Num_documento</span>
                        </div>
                    </div>

